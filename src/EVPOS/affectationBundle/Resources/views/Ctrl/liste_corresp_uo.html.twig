{% extends "EVPOSaffectationBundle::layout.html.twig" %}
{% block title%}Suivi de la migration MOCA{% endblock%}

{% block content %}
<div class="row">
    <div class="col-lg-8">
        <h4>{{ 'uo.corresp'|trans }}</h4>
    </div>
</div>
<table class="table table-striped table-bordered table-hover dataTable no-footer">
    <thead>
        <tr>
            <td class="row sorting_asc">{{ 'uo.codeOld'|trans }}</td>
			<td class="row sorting_asc">{{ 'uo.codeNew'|trans }}</td>
        </tr>

    </thead>
    <tbody>
        {% for corresp in listeCorresp %}
        <tr class="gradeA odd">
			<td>{{corresp.oldCodeUo}}</td>
			<td>
                <div class="row">
                    <div class="col-lg-11">{{corresp.newUo.codeUo}}</div>
                    <div class="col-lg-1"><span class="glyphicon glyphicon-trash"></span></div>
                </div>
            </td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="row">
    <div class="col-lg-12">
        <a class="btn btn-sm btn-default">
            <span class="glyphicon glyphicon-plus"></span>
            {{'app.add'|trans}}
        </a>
    </div>
</div>
{%endblock%}
