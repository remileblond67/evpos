{% extends "EVPOSaffectationBundle::layout.html.twig" %}
{% block title%}{{'uo.sansUtil'|trans}}{% endblock%}

{% block content %}
<div class="row">
    <div class="col-lg-8">
        <h4>{{'uo.sansUtil'|trans}}</h4>
    </div>
</div>

<table class="table table-striped table-bordered table-hover dataTable no-footer sortable">
    <thead>
        <tr>
            <td class="row">{{ 'appli.codeAppli'|trans }}</td>
            <td class="row">{{ 'uo.code'|trans }}</td>
            <td class="row">{{ 'uo.nbUtil'|trans}}</td>
        </tr>

    </thead>
    <tbody>
        {% for uo in listeUo %}
        <tr class="gradeA odd">
          <td>{{uo.appli.codeAppli}}</td>
    			<td>{{uo.codeUo}}</td>
          <td>{{uo.nbUtil}}</td>
    		</tr>
		{% endfor %}
	</tbody>
</table>
{%endblock%}
