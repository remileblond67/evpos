{% extends "EVPOSaffectationBundle::layout.html.twig" %}
{% block title%}Suivi de la migration MOCA{% endblock%}

{% block content %}
<div class="row">
    <div class="col-lg-8">
        <h4>{{ 'ctrl.uoInconnue'|trans }}</h4>
    </div>
</div>
<table class="table table-striped table-bordered table-hover dataTable no-footer sortable">
    <thead>
        <tr>
            <td class="row sorting_asc">{{ 'uo.code'|trans }}</td>
			<td class="row sorting_asc">{{ 'ctrl.nb'|trans }}</td>
        </tr>

    </thead>
    <tbody>
        {% for uo in listeUoInconnue %}
        <tr class="gradeA odd">
			<td>{{uo.codeUo}}</td>
			<td>{{uo.nb}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
{%endblock%}
