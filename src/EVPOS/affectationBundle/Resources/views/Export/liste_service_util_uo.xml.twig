<?xml version="1.0" encoding="UTF-8"?>
<listeDirectionServiceUtilAppli xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    {% for direction in listeDirection %}
    <direction>
        <codeDirection>{{ direction.codeDirection }}</codeDirection>
        <libDirection>{{ direction.libDirection }}</libDirection>
        {% for service in direction.listeServices %}
            DEBUG : {{service.codeService}}
            {% if (service.codeService is not null or service.codeService != "?") and service.nbAgent > 0 %}
            <service>
                <codeService>{{service.codeService}}</codeService>
                <libService>{{service.libService}}</libService>
                <numEnsemble>{{service.numEnsemble}}</numEnsemble>
                <utilisateurs>
                    {% for utilisateur in service.listeUtilisateurs %}
                    <utilisateur>
                        <matUtil>{{ utilisateur.matUtil }}</matUtil>
                        <nomUtil>{{ utilisateur.nomUtil }}</nomUtil>
                        <niveauVIP>{{ utilisateur.niveauVIP }}</niveauVIP>
                        <uos>
                            {% for uo in utilisateur.listeAccesUo %}
                            <uo>
                                <codeAppli>{{ uo.uoAcces.appli.codeAppli }}</codeAppli>
                                <nomAppli>{{ uo.uoAcces.appli.nomAppli }}</nomAppli>
                                <natureAppli>{{ uo.uoAcces.appli.natAppli }}</natureAppli>
                                <codeUo>{{ uo.uoAcces.codeUo }}</codeUo>
                                <nomUo>{{ uo.uoAcces.nomUo }}</nomUo>
                                <migMoca>{{ uo.uoAcces.migMoca }}</migMoca>
                                <avancementMoca>{{ uo.uoAcces.avancementMoca }}</avancementMoca>
                                <avancementMocaDetail>{{ uo.uoAcces.avancementMocaDetail }}</avancementMocaDetail>
                                <ancienCitrix>{{ uo.uoAcces.ancienCitrixLong }}</ancienCitrix>
                                <typePoste>{{ uo.uoAcces.typePoste }}</typePoste>
                                <nbUtil>{{ uo.uoAcces.nbUtil }}</nbUtil>
                            </uo>
                            {% endfor %}
                        </uos>
                        <postes>
                            {% for poste in utilisateur.listePostes %}
                            <poste>
                              <avancementMigMoca>{{poste.avancementMigMoca}}</avancementMigMoca>
              								<hostname>{{poste.hostname}}</hostname>
              								<codeMateriel>{{poste.codeMateriel}}</codeMateriel>
              								<categorie>{{poste.categorie}}</categorie>
              								<modele>{{poste.modele}}</modele>
              								<localisation>{{poste.localisation}}</localisation>
                              <typeReseau>{{poste.typeReseau}}</typeReseau>
              								<statut>{{poste.statut}}</statut>
              								<typeUsage>{{poste.typeUsage}}</typeUsage>
                              <licenceW8>{{poste.licenceW8Long}}</licenceW8>
              								<ecrans>
              								{% for ecran in poste.listeEcran %}
              									<ecran>{{ecran}}</ecran>
              								{% endfor %}
              								</ecrans>
                              <commentaire>{{poste.commentaire}}</commentaire>
                            </poste>
                            {% endfor %}
                        </postes>
                    </utilisateur>
                    {% endfor %}
                </utilisateurs>
            </service>
            {% endif %}
        {% endfor %}
    </direction>
    {% endfor %}
</listeDirectionServiceUtilAppli>
