<?xml version="1.0" encoding="UTF-8"?>
<listeDirectionServiceUtilAppli xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    {% for direction in listeDirection %}
    <direction>
        <codeDirection>{{ direction.codeDirection }}</codeDirection>
        <libDirection>{{ direction.libDirection }}</libDirection>
        {% for service in direction.listeServices %}
            {% if service.codeService is not null and service.nbAgent > 0 %}
            <service>
                <codeService>{{service.codeService}}</codeService>
                <libService>{{service.libService}}</libService>
                <utilisateurs>
                    {% for utilisateur in service.listeUtilisateurs %}
                    <utilisateur>
                        <matUtil>{{utilisateur.matUtil }}</matUtil>
                        <nomUtil>{{ utilisateur.nomUtil }}</nomUtil>
                        <uos>
                            {% for uo in utilisateur.listeAccesUo %}
                            <uo>
                                <codeAppli>{{ uo.uoAcces.appli.codeAppli }}</codeAppli>
                                <nomAppli>{{ uo.uoAcces.appli.nomAppli }}</nomAppli>
                                <natureAppli>{{ uo.uoAcces.appli.natAppli }}</natureAppli>
                                <codeUo>{{ uo.uoAcces.codeUo }}</codeUo>
                                <nomUo>{{ uo.uoAcces.nomUo }}</nomUo>
                                <avancementMoca>{{ uo.uoAcces.avancementMoca }}</avancementMoca>
                                <typePoste>{{ uo.uoAcces.typePoste }}</typePoste>
                                <migMoca>{{ uo.uoAcces.migMoca }}</migMoca>
                            </uo>
                            {% endfor %}
                        </uos>
                    </utilisateur>
                    {% endfor %}
                </utilisateurs>
            </service>
            {% endif %}
        {% endfor %}
    </direction>
    {% endfor %}
</listeDirectionServiceUtilAppli>