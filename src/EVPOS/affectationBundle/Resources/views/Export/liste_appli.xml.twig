<?xml version="1.0" encoding="UTF-8"?>
<listeAppli xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
{% for appli in listeAppli %}
<appli>
    <codeAppli>{{appli.codeAppli}}</codeAppli>
    <nomAppli>{{appli.nomAppli}}</nomAppli>
    <descAppli>{{appli.descAppli}}</descAppli>
    <dispoMoca>{{appli.dispoMocaLong}}</dispoMoca>
    <natAppli>{{appli.natAppli}}</natAppli>
    <nbUtil>{{appli.listeAcces| length}}</nbUtil>
    <cpi>
        {% if appli.cpi %}
        {{appli.cpi.nomUtil}}
        {% endif %}
    </cpi>
    <listeUo>
    {% for uo in appli.listeUo %}
        {% if uo.migMoca %}
		<uo>
            <codeUo>{{uo.codeUO }}</codeUo>
            <nomUo>{{ uo.nomUo }}</nomUo>
            <migMoca>{{uo.migMocaLong}}</migMoca>
            <avancementMoca>{{uo.avancementMoca}}</avancementMoca>
        </uo>
		{% endif %}
    {% endfor %}
    </listeUo>
</appli>
{% endfor %}
</listeAppli>
