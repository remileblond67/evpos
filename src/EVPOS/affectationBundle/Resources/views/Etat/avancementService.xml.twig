<?xml version="1.0" encoding="UTF-8"?>
<listeDirection xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
{% for direction in listeDirection %}
    <direction>
        <codeDirection>{{direction.codeDirection}}</codeDirection>
        <libDirection>{{direction.libDirection}}</libDirection>
        <listeService>
        {% for service in direction.listeServices %}
            <service>
                <codeService>{{service.codeService}}</codeService>
                <libService>{{service.libService}}</libService>
                <nbAgentService>{{service.nbAgent}}</nbAgentService>
                <nbPosteService>{{ service.nbPoste }}</nbPosteService>
                <listeUo>
                {% for acces in service.listeAccesUo %}
                    <Uo>
                        <codeAppli>{{acces.uoAcces.appli.codeAppli}}</codeAppli>
                        <codeUo>{{ acces.uoAcces.codeUo }}</codeUo>
                        <ancienCitrix>{{ acces.uoAcces.ancienCitrixLong }}</ancienCitrix>
                        <nbUtil>{{ acces.nbUtil }}</nbUtil>
                        <noteAvancementMoca>{{ acces.uoAcces.noteAvancementMoca }}%</noteAvancementMoca>
                    </Uo>
                {% endfor %}
              </listeUo>
            </service>
        {% endfor %}
        </listeService>
    </direction>
{% endfor %}
</listeDirection>
