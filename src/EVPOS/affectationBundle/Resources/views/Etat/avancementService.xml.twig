<?xml version="1.0" encoding="UTF-8"?>
<listeDirection xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
{% for direction in listeDirection %}
    <direction>
        <codeDirection>{{direction.codeDirection}}</codeDirection>
        <libDirection>{{direction.libDirection}}</libDirection>
        <listeService>
        {% for service in direction.listeServices %}
            <service>
                <codeService>{{service.codeService}}</codeService>
                <libService>{{service.libService}}</libService>
                <nbAgentService>{{service.nbAgent}}</nbAgentService>
                <listeApplication>
                {% for appli in service.listeAcces %}
                    <application>
                        <codeAppli>{{appli.appliAcces.codeAppli}}</codeAppli>
                        <listeUo>
                        {% for uo in appli.appliAcces.listeUO %}
                            <uo>
                                <codeUo>{{ uo.codeUo }}</codeUo>
                                <ancienCitrix>{{ uo.ancienCitrixLong }}</ancienCitrix>
                                <nbUtil></nbUtil>
                                <noteAvancementMoca>{{ uo.noteAvancementMoca }}%</noteAvancementMoca>
                            </uo>
                        {% endfor %}
                        </listeUo>
                        
                    </application>
                {% endfor %}
                </listeApplication>
            </service>
        {% endfor %}
        </listeService>
    </direction>
{% endfor %}
</listeDirection>
