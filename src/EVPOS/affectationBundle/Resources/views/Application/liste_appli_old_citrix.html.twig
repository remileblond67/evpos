{% extends "EVPOSaffectationBundle::layout.html.twig" %}
{% block title%}Suivi de la migration MOCA{% endblock%}

{% block content %}
<div class="row">
    <div class="col-lg-12">
        <h4>{{ 'uo.listeOldCitrix'|trans }}</h4>
    </div>
</div>

<table id="tabAppli" class="table table-striped table-bordered table-hover dataTable no-footer sortable">
    <thead>
        <tr>
            <td class="row sorting_asc">
              {{ 'application.code'|trans }}
            </td>
            <td class="row">
              {{ 'application.nom'|trans }}
            </td>
            <td class="row">
              {{ 'uo.code'|trans }}
            </td>
            <td class="row">
              {{ 'uo.nom'|trans }}
            </td>
        </tr>
    </thead>
    <tbody>
      {% for uo in listeUO %}
      <tr>
        <td>{{appli.codeAppli}}</td>
        <td>{{appli.nomAppli}}</td>
        <td><a href="{{path('evpos_ficheUo', {'codeUo': uo.codeUo})}}">{{uo.codeUo}}</a></td>
        <td>{{uo.nomUo}}</td>
      </tr>
      {% endfor %}
    </tbody>
</table>
{%endblock%}
